{
  "name": "@apipack/spring-boot",
  "version": "0.0.0",
  "description": "Java Spring-BOOT backend adapter for APIPACK.",
  "keywords": [
    "apipack",
    "apipack/spring-boot"
  ],
  "author": "givingwu <givingwu@gmail.com>",
  "homepage": "https://apipack.github.io/spring-boot.html",
  "license": "MIT",
  "main": "lib/index.js",
  "directories": {
    "lib": "lib",
    "test": "__tests__"
  },
  "files": [
    "lib"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/apipack/apipack.git"
  },
  "scripts": {
    "dev": "tsc --watch src/*.ts --outDir ./lib --declaration",
    "clean": "rimraf lib",
    "build": "tsc",
    "lint": "eslint --ext .ts ./src",
    "lint:fix": "eslint --fix --ext .ts ./src",
    "test": "ts-node ./__tests__/*.test.ts",
    "test:dev": "jest --watch",
    "test:jest": "jest",
    "test:jest:dev": "jest --watch"
  },
  "bugs": {
    "url": "https://github.com/apipack/apipack/issues"
  },
  "dependencies": {
    "@apipack/core": "^0.0.0",
    "@types/node": "^18.11.17",
    "java-parser": "^2.0.3"
  },
  "devDependencies": {
    "@types/node": "^18.11.17"
  }
}
